package com.sharad.quizbowlclassifier;

import java.util.ArrayList;
import java.util.List;

import com.sharad.quizbowlclassifier.cloud.Tag;
import com.sharad.quizbowlclassifier.cloud.TagCloud;

public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		List<Tag<String>> l = new ArrayList<Tag<String>>();
		l.add(new Tag<String>("Hey", 3));
